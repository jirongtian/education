<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mappers 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.ntyx.vedu.mapper.TitleMapper">    <resultMap id="BaseResultMap" type="com.ntyx.vedu.entity.Title">        <id column="title_id" property="id"/>        <result column="title_name" property="titleName" />        <result column="course_id" property="courseId" />        <result column="course_name" property="courseName" />    </resultMap>    <!-- &lt;!&ndash;更新&ndash;&gt;-->    <update id="update" parameterType="com.ntyx.vedu.entity.Title">        update pe_title        set title_name = #{titleName},        course_id=#{courseId}        course_name=#{courseName}        where title_id = #{id}    </update>    <!-- //查询全部-->    <select id="findAll"  resultMap="BaseResultMap">        select * from pe_title    </select>    <!--&lt;!&ndash;保存&ndash;&gt;-->    <insert id="save" parameterType="com.ntyx.vedu.entity.Title">        insert into pe_title ( title_name,course_id,course_name)        values (#{titleName},#{courseId},#{courseName})    </insert>    <!-- &lt;!&ndash;根据id查询&ndash;&gt;-->    <select id="findById" parameterType="integer" resultMap="BaseResultMap">        select  * from pe_title where title_id = #{id}    </select>    <!-- &lt;!&ndash;根据id删除&ndash;&gt;-->    <delete id="deleteById" parameterType="integer">        delete from pe_title where title_id = #{id}    </delete></mapper>